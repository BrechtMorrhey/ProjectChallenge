        <TextBox x:Name="invulTextBox" HorizontalAlignment="Left" Height="23" Margin="51,60,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="372"/>
        <Button x:Name="volgendeButton" Content="Volgende Vraag" HorizontalAlignment="Left" Margin="51,272,0,0" VerticalAlignment="Top" Width="99" Click="volgendeButton_Click"/>
        <Button x:Name="klaarButton" Content="Klaar" HorizontalAlignment="Left" Margin="250,272,0,0" VerticalAlignment="Top" Width="75" Click="klaarButton_Click"/>
        <Button x:Name="vorigeButton" Content="Vorige Vraag" HorizontalAlignment="Left" Margin="51,296,0,0" VerticalAlignment="Top" Width="99" Click="vorigeButton_Click"/>
        <ListBox x:Name="invulListBox" HorizontalAlignment="Left" Height="184" Margin="51,83,0,0" VerticalAlignment="Top" Width="372"/>

    </Grid>
</Window>
